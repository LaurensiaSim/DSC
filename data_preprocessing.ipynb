   "source": [
    "from imblearn.over_sampling import SMOTE\n",
    "\n",
    "# Initialize smote\n",
    "oversampling=SMOTE()\n",
    "\n",
    "# fit the X,y dataset\n",
    "X,y=oversampling.fit_resample(data,label)\n",
    "X.shape,y.shape"
